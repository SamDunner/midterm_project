<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Node Skeleton</title>

    <% include partials/_node %>
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>



  </head>

  <body>
  <% include partials/_navbar %>

    <button class="btn" id="toggle_list" aria-hidden="true" type="button">My Lists</button>
    <div class="savePosition" onclick="savePosition()">
      <img  src="/images/plus.svg" >
    </div>


    <div id="map"></div>
    <div id="capture"></div>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB30sSupKcH81MFrDMtaX_cm21V18JR18Y&callback=initMap"
    async defer></script>

    <input type="text" id="name" placeholder="Name"><br>
    <select id="rating">
      <option disabled selected value>Rating</option>
      <option value="1">1</option>
      <option value="2">2</option>
      <option value="3">3</option>
      <option value="4">4</option>
      <option value="5">5</option>
    </select>

    <select id="style">
      <option disabled selected value>Style</option>
      <option value="mexican">Mexican</option>
      <option value="asian varied">Asian varied</option>
      <option value="thai">Thai</option>
      <option value="fastFood">Fast food</option>
      <option value="other">Other</option>
    </select>

    <div class="lists">
      <h1>FAVOURITE PLACES</h1>
      <ul>
        <li>Mcdonalds</li>
        <li>Freshnoodles</li>
        <li>The warehouse</li>
        <li>Subway</li>
      </ul>

    </div>


  </body>

</html>
